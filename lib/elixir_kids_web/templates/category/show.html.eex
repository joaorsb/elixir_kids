<h1>Show Category</h1>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @category.title %>
  </li>

  <li>
    <strong>Slug:</strong>
    <%= @category.slug %>
  </li>

  <li>
    <strong>Type:</strong>
    <%= @category.type %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.category_path(@conn, :edit, @category) %></span>
<span><%= link "Back", to: Routes.category_path(@conn, :index) %></span>
